@model Wfm.Domain.Features.FileManager.GetFiles.GetFilesResult

@{
    ViewData["Title"] = "Files";
}

<h1 class="display-4">@ViewBag.LocationName</h1>

<table>
    <tr>
        <th>Name</th>
        <th>Type</th>
    </tr>
    @foreach (var entry in Model.Entries)
    {
        <tr>
            <td>@entry.Name</td>
            <td>@entry.Type</td>
        </tr>
    }
</table>
